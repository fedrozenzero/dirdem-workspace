{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "fuse-bit": [
        "dist/fuse-bit/fuse-bit",
        "dist/fuse-bit"
      ],
      "dirdem-common": [
        "dist/dirdem-common/dirdem-common",
        "dist/dirdem-common"
      ],
      "technical-specifications": [
        "dist/technical-specifications/technical-specifications",
        "dist/technical-specifications"
      ]
    }
  }
}
